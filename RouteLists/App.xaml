<Application x:Class="RouteLists.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RouteLists"
             StartupUri="View\Windows\WindowMain.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="\Resources\Dictionaries\Images.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Colors.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Brushes.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Fonts.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Styles\ImageStyles.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Styles\TextBoxStyles.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Styles\LabelStyles.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Styles\DatePickerStyles.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Styles\ButtonStyles.xaml"/>
                <ResourceDictionary Source="\Resources\Dictionaries\Templates\ButtonTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
